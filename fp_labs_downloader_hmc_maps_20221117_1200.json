{
  "generic": {
    "path_home": null,
    "path_root_data_static_source": "/hydro/data/data_static/",
    "path_root_data_static_destination": "$HOME/fp_labs_datasets/data_static/",
    "path_root_data_dynamic_source_results": "/hydro/archive/",
    "path_root_data_dynamic_source_forcing": "/hydro/data/data_dynamic/outcome/",
    "path_root_data_dynamic_destination_results": "$HOME/fp_labs_datasets/data_dynamic/maps/results/",
    "path_root_data_dynamic_destination_forcing": "$HOME/fp_labs_datasets/data_dynamic/maps/forcing/",
    "domain_name": "marche",
    "analysis_name" : "evento",
    "machine": {
      "__comment__": "type: [local, remote]",
      "type": "remote",
      "user": "user",
      "ip": "10.198.26.21"
    },
    "transfer_methods": {
      "rsync" : {
        "command_line": "rsync -avr --ignore-existing {machine_address}:{path_source} {path_destination}"}
    }
  },
  "template": {
    "transfer":{
      "path_source": "string_path_source",
      "path_destination": "string_path_destination",
      "machine_address": "string_server_address"
    },
    "path": {
      "path_root_data_static_source": "string_path_root_data_static_source",
      "path_root_data_static_destination": "string_path_root_data_static_destination",
      "path_root_data_dynamic_source_results": "string_path_root_data_dynamic_source_results",
      "path_root_data_dynamic_source_forcing": "string_path_root_data_dynamic_source_forcing",
      "path_root_data_dynamic_destination_results": "string_path_root_data_dynamic_destination_results",
      "path_root_data_dynamic_destination_forcing": "string_path_root_data_dynamic_destination_forcing"
    },
    "time": {
      "data_source_datetime": "%Y%m%d%H%M",
      "data_source_sub_path": "%Y/%m/%d/%H",
      "data_destination_datetime": "%Y%m%d%H%M",
      "data_destination_sub_path": "%Y%m%d_%H"
    },
    "data" : {
      "ensemble_id": "string_ensemble_id"
    }
  },
  "data" : {
    "static": {
      "source": {
        "terrain": {
          "folder_name": "{path_root_data_static_source}/gridded_marche/",
          "file_name": "marche.dem.txt",
          "transfer_method": "rsync",
          "flag_active": true,
          "flag_update": true
        },
        "river_network": {
          "folder_name": "{path_root_data_static_source}/gridded_marche/",
          "file_name": "marche.choice.txt",
          "transfer_method": "rsync",
          "flag_active": true,
          "flag_update": true
        },
        "sections": {
          "folder_name": "{path_root_data_static_source}/shapefile_marche/",
          "file_name": "fp_sections_marche.shp",
          "transfer_method": "rsync",
          "flag_active": true,
          "flag_update": true
        }
      },
      "destination": {
        "terrain": {
          "folder_name": "{path_root_data_static_destination}/gridded_marche/",
          "file_name": "marche.dem.txt"
        },
        "river_network": {
          "folder_name": "{path_root_data_static_destination}/gridded_marche/",
          "file_name": "marche.choice.txt"
        },
        "sections": {
          "folder_name": "{path_root_data_static_destination}/shapefile_marche/",
          "file_name": "fp_sections_marche.shp"
        }
      }
    },
    "dynamic": {
      "source": {
        "weather_stations": {
          "folder_name": "{path_root_data_dynamic_source_results}/weather_stations_realtime_marche/{data_source_sub_path}/gridded/",
          "file_name": "hmc.output-grid.{data_source_datetime}.nc.gz",
          "time_reference": "2022-11-17 12:00",
          "time_period": 10,
          "time_frequency": "H",
          "transfer_method": "rsync",
          "flag_active": true,
          "flag_update": true,
          "ensemble_n": null
        },
        "domain_forcing": {
          "folder_name": "{path_root_data_dynamic_source_data_forcing}/obs/weather_stations/{data_source_sub_path}/",
          "file_name": "ws.db.{data_source_datetime}.nc.gz",
          "time_reference": "2022-11-17 12:00",
          "time_period": 10,
          "time_frequency": "H",
          "transfer_method": "rsync",
          "flag_active": true,
          "flag_update": true,
          "ensemble_n": null
        }
      },
      "destination": {
        "weather_stations": {
          "folder_name": "{path_root_data_dynamic_destination_results}/weather_stations_realtime/{data_destination_sub_path}/",
          "file_name": "hmc.output-grid.{data_source_datetime}.nc.gz"
        },
        "domain_forcing": {
          "folder_name": "{path_root_data_dynamic_destination_forcing}/weather_stations_realtime/{data_destination_sub_path}/",
          "file_name": "ws.db.{data_source_datetime}.nc.gz"
        }
      }
    }
  },
  "log": {
    "folder_name": "$HOME/fp_labs_apps/tmp/",
    "file_name": "fp_data_downloader_maps_20221117_12.txt"
  }
}
